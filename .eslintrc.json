{
    "extends": "airbnb",
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": true
            }
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "eol-last": [
            "error",
            "never"
        ],
        "eqeqeq": 2,
        "max-len": [
            2,
            100
        ],
        "@typescript-eslint/no-unused-vars": 2
    }
}